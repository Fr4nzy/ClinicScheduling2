<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    tools:context=".FindadoctorActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="500dp"
        android:layout_height="200dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/doctorsprofilepicture" />

    <ImageView
        android:id="@+id/backbutton"
        android:layout_width="750dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backicon" />

    <ImageView
        android:id="@+id/locationicon"
        android:layout_width="750dp"
        android:layout_height="30dp"
        android:layout_marginTop="324dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/locationicon" />

    <ImageView
        android:id="@+id/contacticon"
        android:layout_width="750dp"
        android:layout_height="30dp"
        android:layout_marginTop="444dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/contacticon" />

    <ImageView
        android:id="@+id/calendaricon"
        android:layout_width="750dp"
        android:layout_height="30dp"
        android:layout_marginTop="552dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.769"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/calendaricon" />

    <ImageView
        android:id="@+id/textbox1"
        android:layout_width="450dp"
        android:layout_height="100dp"
        android:layout_marginTop="292dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/textbox" />

    <TextView
        android:id="@+id/doctorslname"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="12dp"
        android:text="Lorem Ipsum"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textFontWeight="@integer/material_motion_duration_medium_1"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/doctorslocation"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:text="Input Doctor's Location"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textFontWeight="@integer/material_motion_duration_medium_1"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/textbox2"
        android:layout_width="450dp"
        android:layout_height="100dp"
        android:layout_marginTop="408dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/textbox" />

    <TextView
        android:id="@+id/doctorsnumber"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="220dp"
        android:text="Input Doctor's Phone \nnumber"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textFontWeight="@integer/material_motion_duration_medium_1"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/textbox3"
        android:layout_width="450dp"
        android:layout_height="100dp"
        android:layout_marginTop="524dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/textbox" />

    <TextView
        android:id="@+id/doctorsavailability"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="336dp"
        android:text="View Doctor's Schedule "
        android:textAlignment="center"
        android:textColor="#000000"
        android:textFontWeight="@integer/material_motion_duration_medium_1"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/checkdoctorsavailability"
        android:layout_width="650dp"
        android:layout_height="70dp"
        android:layout_marginTop="648dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/checkavailability" />

    <TextView
        android:id="@+id/checkdoctorsavailability1"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="496dp"
        android:text="Click here to schedule an appointment."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textFontWeight="@integer/material_motion_duration_medium_1"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.clinicscheduling">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClinicScheduling"
        tools:targetApi="31">
        <activity
            android:name=".PatientsinfoActivity"
            android:exported="false"
            android:label="@string/title_activity_patientsinfo"
            android:theme="@style/Theme.ClinicScheduling.NoActionBar" />
        <activity
            android:name=".DentistinfoActivity"
            android:exported="false"
            android:label="@string/title_activity_dentistinfo"
            android:theme="@style/Theme.ClinicScheduling.NoActionBar" />
        <activity
            android:name=".SelectyourdoctorActivity"
            android:exported="false"
            android:label="@string/title_activity_selectyourdoctor"
            android:theme="@style/Theme.ClinicScheduling.NoActionBar" />
        <activity
            android:name=".FindadoctorActivity"
            android:exported="false"
            android:label="@string/title_activity_findadoctor"
            android:theme="@style/Theme.ClinicScheduling.NoActionBar" />
        <activity
            android:name=".login"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>